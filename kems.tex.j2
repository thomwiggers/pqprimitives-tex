% Ignore wrong-length-of-dash warnings
% chktex-file 8
%
\makeatletter
\newcommand{\@DefineSingleKemProperty}[3]{%
\csdef{kem-#1-#2}{#3}%
}
\newcommand{\@DefineAllKemProperties}[8]{%
    \@DefineSingleKemProperty{#1}{name}{#1}
    \@DefineSingleKemProperty{#1}{display}{#2}
    \@DefineSingleKemProperty{#1}{level}{#3}
    \@DefineSingleKemProperty{#1}{status}{#4}
    \@DefineSingleKemProperty{#1}{assumption}{#5}
    \@DefineSingleKemProperty{#1}{pk}{#6}
    \@DefineSingleKemProperty{#1}{ct}{#7}
    \@DefineSingleKemProperty{#1}{sk}{#8}
}
\makeatother

\newcommand{\KemProperty}[2]{\csuse{kem-#1-#2}}

\makeatletter
<% for kem in kems %>
\@DefineAllKemProperties{<< kem['name'] >>}{<< kem['display'] >>}{<< kem['level'] >>}{<< kem['status'] >>}{<< kem['assumption'] >>}{<< kem['pk'] >>}{<< kem['ct'] >>}{<< kem['sk'] >>}
<% endfor %>
\makeatother
